  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@freshbooks/api": "^4.1.0",
    "commander": "^14.0.3",
    "conf": "^15.1.0",
    "open": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
